{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2019",
        "lib": ["ES2019", "dom"],
        "rootDir": "extension",
        "outDir": "out",
        "esModuleInterop": true,
        "noImplicitAny": false,
        "sourceMap": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["extension"],
    "exclude": ["node_modules", "test-resources", ".vscode-test"]
}
