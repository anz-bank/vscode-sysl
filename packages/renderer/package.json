{
  "name": "@anz-bank/vscode-sysl-renderer",
  "displayName": "Sysl",
  "description": "Write Sysl specs easily with instant visual feedback.",
  "version": "0.55.0",
  "categories": [
    "Programming Languages"
  ],
  "author": "ANZ Bank",
  "publisher": "anz-bank",
  "repository": {
    "type": "git",
    "url": "https://github.com/anz-bank/vscode-sysl"
  },
  "license": "Apache-2.0",
  "sideEffects": false,
  "scripts": {
    "prebuild": "yarn install --network-timeout 300000",
    "start": "craco start",
    "build": "craco build && browserify -e build/static/js/main.js -o build/static/js/main.js",
    "test": "craco test",
    "eject": "craco eject",
    "pack": "pack",
    "prepack": "mkdir -p dist && cp package.json ../../README.md ../../LICENSE dist/ && sed -i'' -e 's|dist/||g' dist/package.json"
  },
  "dependencies": {
    "@anz-bank/vscode-sysl-model": "^0.55.0",
    "@craco/craco": "^6.4.5",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@mui/icons-material": "^5.0.1",
    "@mui/lab": "^5.0.0-alpha.49",
    "@mui/material": "^5.0.2",
    "@mui/styles": "^5.0.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "gojs": "^2.1",
    "gojs-react": "^1.0.10",
    "lodash": "^4.17.21",
    "opentable": "^6",
    "opentable-react": "^6",
    "react": "^17",
    "react-dom": "^17",
    "react-scripts": "^4"
  },
  "devDependencies": {
    "@types/jest": "^26",
    "@types/lodash": "^4",
    "@types/node": "^16",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "browserify": "^17",
    "canvas": "^2",
    "jest": "^26",
    "typescript": "^4"
  },
  "resolutions": {
    "@types/react": "^17",
    "@types/react-dom": "^17"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
