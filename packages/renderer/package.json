    "react": "^17",
    "react-dom": "^17",
    "react-scripts": "*"
  },
  "devDependencies": {
    "@types/jest": "^29",
    "@types/lodash": "^4",
    "@types/node": "^20",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "browserify": "^17",
    "canvas": "^3",
    "jest": "^29",
    "typescript": "^5"
  },
  "resolutions": {
    "@types/react": "^17",
    "@types/react-dom": "^17"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
